<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.triibe.triibeuserapp">

    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/westfield_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".auth.AuthUiActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".trackData.TrackDataActivity"
            android:label="@string/title_activity_track_data" />

        <activity
            android:name=".takeSurvey.TakeSurveyActivity"
            android:label="@string/title_activity_take_survey"
            android:parentActivityName=".trackData.TrackDataActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.triibe.triibeuserapp.trackData.TrackDataActivity" />
        </activity>

    </application>

</manifest>